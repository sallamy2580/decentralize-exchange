// +prop AppID = '1'
// +prop Conditions = 'ContractConditions("MainCondition")'
contract NewAppParam {
    data {
        ApplicationId int
        Name string
        Value string
        Conditions string
    }

    action {
        DBInsert("app_params", {app_id: $ApplicationId, name: $Name, value: $Value,
              conditions: $Conditions})
    }
}
