// +prop AppID = '1'
// +prop Conditions = 'ContractConditions("MainCondition")'
contract NewLang {
    data {
        ApplicationId int
        Name string
        Trans string
    }

    conditions {
        }

        EvalCondition("parameters", "changing_language", "value")
    }

    action {
        CreateLanguage($Name, $Trans)
    }
}
