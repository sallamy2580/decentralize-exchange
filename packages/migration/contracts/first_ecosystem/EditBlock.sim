// +prop AppID = '1'
// +prop Conditions = 'ContractConditions("MainCondition")'
contract EditBlock {
    data {
        Id int
        Value string "optional"
        Conditions string "optional"
    }
    func onlyConditions() bool {
            pars["conditions"] = $Conditions
        }
        if pars {
            DBUpdate("blocks", $Id, pars)
        }
    }
}
