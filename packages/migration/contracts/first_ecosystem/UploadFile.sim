// +prop AppID = '1'
// +prop Conditions = 'ContractConditions("MainCondition")'
contract UploadFile {
    data {
        ApplicationId int
        Data file
    action {
        $Id = @1UploadBinary("ApplicationId,Name,Data,DataMimeType", $ApplicationId, $Name, $Body, $DataMimeType)
        $result = $Id
    }
}
