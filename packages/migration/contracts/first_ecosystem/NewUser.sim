// +prop AppID = '1'
// +prop Conditions = 'ContractConditions("NodeOwnerCondition")'
contract NewUser {
	data {
		NewPubkey string
	}
	conditions {
		$id = PubToID($NewPubkey)
		if $id == 0 {
			error "Wrong pubkey"
		}
			"pub": $pub,
			"amount": $amount,
			"ecosystem": 1
		})
	}
}
