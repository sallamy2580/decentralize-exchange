// +prop AppID = '1'
// +prop Conditions = 'ContractConditions("MainCondition")'
contract UpdateSysParam {
     data {
        Name string
        Value string
        Conditions string "optional"
     }
        var params map
        params["Value"] = $Value
        CallContract($Name, params)
        
        DBUpdateSysParam($Name, $Value, $Conditions)
     }
}
